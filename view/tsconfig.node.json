{
  "extends": "@tsconfig/node20/tsconfig.json",
  "include": ["types", "env.d.ts", "src/**/*", "src/**/*.ts","src/**/*.js", "src/**/*.d.ts", "src/**.vue","src/**/*.vue","src/**/**/*.vue", "src/**/*.json", "types/*.d.ts", "public/device"],
  "exclude": ["src/**/__tests__/*", "node_modules"],
  "compilerOptions": {
    "lib": ["ES2018", "DOM"],
    "composite": true,
    "noEmit": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "module": "ESNext",
    "moduleResolution": "Node",
    "types": ["vite/client"],
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    },
    "resolveJsonModule": true
  }
}
